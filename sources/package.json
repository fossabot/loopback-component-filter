{
    "name": "loopback-component-filter",
    "version": "0.1.0",
    "description": "Loopback component models filter",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "clean": "lb-clean dist *.tsbuildinfo",
        "prebuild": "npm run clean",
        "build": "lb-tsc --outDir dist",
        "build:watch": "lb-tsc --watch --outDir dist",
        "start": "node .",
        "start:watch": "nodemon .",
        "test": "lb-mocha --allow-console-logs \"test/**/*.test.ts\"",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/loopback4/loopback-component-filter"
    },
    "author": "KoLiBer",
    "license": "MIT",
    "files": [
        "README.md",
        "index.js",
        "index.d.ts",
        "dist"
    ],
    "dependencies": {
        "@loopback/boot": "^2.1.2",
        "@loopback/context": "^3.6.0",
        "@loopback/core": "^2.4.2",
        "@loopback/repository": "^2.3.0",
        "tslib": "^1.10.0"
    },
    "devDependencies": {
        "@loopback/build": "^5.3.0",
        "@loopback/testlab": "^3.1.2",
        "@types/node": "^10.17.21",
        "nodemon": "^2.0.3",
        "source-map-support": "^0.5.19",
        "typescript": "~3.8.3"
    }
}
