{
    "name": "loopback-component-filter",
    "version": "0.5.0",
    "description": "Loopback component models filter",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "clean": "lb-clean dist *.tsbuildinfo",
        "prebuild": "npm run clean",
        "build": "lb-tsc --outDir dist",
        "build:watch": "lb-tsc --watch --outDir dist",
        "start": "node .",
        "start:watch": "nodemon .",
        "test": "lb-mocha --allow-console-logs \"test/**/*.test.ts\"",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/loopback4/loopback-component-filter"
    },
    "author": "KoLiBer",
    "license": "MIT",
    "files": [
        "README.md",
        "index.js",
        "index.d.ts",
        "dist"
    ],
    "dependencies": {
        "@loopback/boot": "^2.3.1",
        "@loopback/context": "^3.8.1",
        "@loopback/core": "^2.7.0",
        "@loopback/repository": "^2.5.1",
        "tslib": "^1.10.0"
    },
    "devDependencies": {
        "@loopback/build": "^5.4.1",
        "@loopback/testlab": "^3.1.5",
        "@types/node": "^10.17.24",
        "nodemon": "^2.0.4",
        "ts-node": "^8.10.1",
        "typescript": "~3.9.3"
    }
}
